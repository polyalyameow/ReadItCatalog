{
    "version": 2,
    "builds": [
      {
        "src": "server/api/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  